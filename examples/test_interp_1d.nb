Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"cubic", "[", 
    RowBox[{"p0_", ",", "p1_", ",", "p2_", ",", "p3_", ",", "x_"}], "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "0.5"}], "*", "p0"}], "+", 
       RowBox[{"1.5", "*", "p1"}], "-", 
       RowBox[{"1.5", "*", "p2"}], "+", 
       RowBox[{"0.5", "*", "p3"}]}], ")"}], "*", 
     SuperscriptBox["x", "3"]}], "+", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"p0", "-", 
       RowBox[{"2.5", "*", "p1"}], "+", 
       RowBox[{"2.0", "*", "p2"}], "-", 
       RowBox[{"0.5", "*", "p3"}]}], ")"}], "*", 
     SuperscriptBox["x", "2"]}], "+", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        RowBox[{"-", "0.5"}], "*", "p0"}], "+", 
       RowBox[{"0.5", "*", "p2"}]}], ")"}], "*", "x"}], "+", "p1"}]}], 
  ";"}]], "Input",
 CellLabel->"In[5]:=",ExpressionUUID->"e04d1622-d191-45c2-ae8b-69e21c0e93a5"]
}, Open  ]],

